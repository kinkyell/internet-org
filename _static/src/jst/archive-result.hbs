<div class="resultsList-list-item">
    <div class="feature feature_tight">
        <div class="feature-hd">
            <h2 class="hdg hdg_3">{{title}}</h2>
        </div>
        <div class="feature-date">
            <div class="hdg hdg_6 mix-hdg_italic mix-hdg_gray">{{date}}</div>
        </div>
        <div class="feature-bd">
            <p class="bdcpy">{{desc}}</p>
        </div>
        <div class="feature-cta">
            <a href="{{url}}" class="link link_sm js-stateLink" data-type="titled" data-title="{{title}}" data-desc="{{desc}}">Read More</a>
        </div>
    </div>
</div>
